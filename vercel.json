{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "server.js"
    },
    {
      "src": "/start",
      "dest": "server.js"
    },
    {
      "src": "/callback",
      "dest": "server.js"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)\\.(js|css|png|jpg|jpeg|gif|ico)",
      "dest": "public/$1.$2"
    }
  ]
}
